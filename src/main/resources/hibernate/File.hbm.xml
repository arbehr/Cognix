<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="cognitivabrasil.repositorio.data.entities.Files" table="files">
    <meta attribute="class-description"> 
    </meta>
    <id column="id" name="id" type="int">
      <generator class="sequence">
        <param name="sequence">files_id_seq</param>
      </generator>
    </id>
    <property name="name" not-null="true" type="string"/>
    <property name="location" type="string"/>
    <property column="content_type" name="contentType" type="string"/>
    <property column="random_name" name="randomName" type="string"/>
    <property name="size" type="long"/>
    <many-to-one class="cognitivabrasil.repositorio.data.entities.Document" column="document" fetch="select" name="document" not-null="true"/>
  </class>
</hibernate-mapping>
